{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "npm-watch": "^0.11.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js, jsx"
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "server": "nodemon src/index.js",
    "react": "react-scripts start",
    "dev": "concurrently \"npm run server\" \"npm run react\" ",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "esbuild": "^0.12.14",
    "nodemon": "^2.0.15",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1"
  }
}
